                <span class="o_field_char o_field_widget o_quick_editable o_readonly_modifier o_required_modifier item" data-class="oe_title h1 span" title="Edit: Title" data-name="Title">Title</span>
              </h1>
            </div>
            <div class="o_group">
              <table class="o_group o_inner_group o_group_col_6">
                <tbody>
                  <tr>
                    <td class="o_td_label"><label class="o_form_label o_required_modifier item" data-class="o_form_label" title="Edit: Form Label" data-name="Form Label">Label</label></td>
                    <td style="width: 100%;"><a class="o_form_uri o_field_widget o_quick_editable o_required_modifier"><span class="item" data-class="o_form_uri > span:first-child"  title="Edit: Link many2one" data-name="Link many2one">Link</span></a></td>
                  </tr>
                  <tr>
                      <td class="o_td_label"/>
                      <td style="width: 100%;"/>
                  </tr>
                  <tr>
                      <td class="o_td_label"/>
                      <td style="width: 100%;"/>
                  </tr>
                </tbody>
              </table>
              <table class="o_group o_inner_group o_group_col_6">
                <tbody>
                  <tr>
                    <td class="o_td_label"><label class="o_form_label o_required_modifier item" data-class="o_form_label" title="Edit: Form Label" data-name="Form Label">Label</label></td>
                    <td style="width: 100%;">
                        <input class="o_field_char o_field_widget o_quick_editable o_input item" data-class="o_input"  title="Edit: Form Input" data-name="Form Input" type="text"/>
                    </td>
                  </tr>
                  <tr>
                      <td class="o_td_label"/>
                      <td style="width: 100%;"/>
                  </tr>
                  <tr>
                      <td class="o_td_label"/>
                      <td style="width: 100%;"/>
                  </tr>
                </tbody>
              </table>
            </div>
            <div class="o_notebook item" data-class="o_notebook" title="Edit: Form Notebook" data-name="Form Notebook">
              <div class="o_notebook_headers item" data-class="o_notebook_headers" title="Edit: Form Notebook Header" data-name="Form Notebook Header">
