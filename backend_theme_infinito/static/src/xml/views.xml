    <t t-name="ThemeStudio.kanban">
        Kanban grouped
        <div class="o_kanban_view o_kanban_small_column o_opportunity_kanban ui-sortable o_kanban_grouped">
            <div class="o_kanban_group ui-sortable o_kanban_has_progressbar item" data-class="o_kanban_view .o_kanban_group" data-name="Kanban Group" title="Edit: Kanban Group" data-id="1" role="feed" aria-labelledby="kanban_header_1">
                <div class="o_kanban_header">
                    <div class="o_kanban_header_title item" data-class="o_kanban_header_title" data-name="Kanban Header Title Section" title="Edit: Kanban Header Title Section">
                        <span id="#kanban_header_1" class="o_column_title item" data-class="o_kanban_header_title .o_column_title" data-name="Kanban Header Title" title="Edit: Kanban Header Title">Stage</span>
                        <span class="o_kanban_config dropdown">
                            <a class="dropdown-toggle o-no-caret" data-toggle="dropdown" ><i class="fa fa-gear" role="img" aria-label="Settings" title="Settings"/></a>
                        </span>
                        <span class="o_kanban_quick_add"><i class="fa fa-plus" role="img" aria-label="Quick add" title="Quick add"/></span>
                    </div>
                    <div class="o_kanban_counter">
                        <div class="o_kanban_counter_progress progress item" data-class="o_kanban_counter .o_kanban_counter_progress" data-name="Kanban Header Progressbar" title="Edit: Kanban Header Progressbar">
                            <div class="progress-bar bg-success" data-filter="planned" data-original-title="0 Planned" aria-valuemin="0" aria-valuemax="100" aria-valuenow="0" role="progressbar" style=""/>
                            <div class="progress-bar bg-warning" data-filter="today" data-original-title="0 Today" aria-valuemin="0" aria-valuemax="100" aria-valuenow="0" role="progressbar" style=""/>
                            <div class="progress-bar bg-danger o_bar_has_records" data-filter="overdue" data-original-title="1 Overdue" aria-valuemin="0" aria-valuemax="2" aria-valuenow="1" role="progressbar" style="width: 50%; max-width: 94%; min-width: 6%;"/>
                            <div class="progress-bar bg-muted o_bar_has_records" data-filter="__false" data-original-title="1 Other" aria-valuemin="0" aria-valuemax="2" aria-valuenow="1" role="progressbar" style="width: 50%; max-width: 94%; min-width: 6%;"/>
                        </div>
                        <div class="o_kanban_counter_side item" data-class="o_kanban_counter .o_kanban_counter_side" data-name="Kanban Header Counter" title="Edit: Kanban Header Counter"><span>$</span><b>00,000</b></div>
                    </div>
                </div>
                Muted Card
                <div class="oe_kanban_color_0 oe_kanban_global_click oe_kanban_card d-flex flex-column o_kanban_record oe_kanban_card_muted item" data-class="oe_kanban_card_muted" data-name="Kanban Card Muted" title="Edit: Kanban Card Muted" tabindex="0" role="article">
                    <div class="o_dropdown_kanban dropdown">
                        <a class="dropdown-toggle o-no-caret btn" role="button" data-toggle="dropdown" data-display="static"  aria-label="Dropdown menu" title="Dropdown menu">
                            <span class="fa fa-ellipsis-v"/>
                        </a>
                    </div>
                    <div class="oe_kanban_content flex-grow-1">
                        <div class="o_kanban_record_title oe_kanban_details item" data-class="oe_kanban_card_muted .o_kanban_record_title" data-name="Kanban Card Record Title" title="Edit: Kanban Record Title">
                            <strong><span>Title</span></strong>
                        </div>
                        <div class="o_kanban_record_subtitle item" data-class="oe_kanban_card_muted .o_kanban_record_subtitle" data-name="Kanban Record Details" title="Edit: Kanban Record Details">
                            <span class="o_field_monetary o_field_number o_field_widget o_quick_editable" name="expected_revenue">$&amp;nbsp;00,000.00</span>
                        </div>
                        <div>
                            <div class="o_field_many2manytags o_field_widget o_kanban_tags" name="tag_ids">
                                <span class="o_tag o_tag_color_1"><span/>Tags</span>
                            </div>
                        </div>
                    </div>
                    <div class="oe_kanban_footer item" data-class="oe_kanban_card_muted .oe_kanban_footer" data-name="Kanban Card Footer" title="Edit: Kanban Card Footer">
                        <div class="o_kanban_record_bottom">
                            <div class="oe_kanban_bottom_left item" data-class="oe_kanban_card_muted .oe_kanban_bottom_left" data-name="Kanban Card Footer Left" title="Edit: Kanban Card Footer Left">
                                <div role="radiogroup" class="o_priority o_field_widget" name="priority" aria-label="Priority">
                                    <a role="radio" aria-checked="true" tabindex="0" data-index="1" class="o_priority_star fa fa-star item" data-class="oe_kanban_card_muted .o_priority_star.fa-star" data-name="Kanban Checked Priority Start" title="Edit: Kanban Checked Priority Start"/>
                                    <a role="radio" aria-checked="false" aria-label="High" tabindex="-1" data-index="2" class="o_priority_star fa fa-star-o item" data-class="oe_kanban_card_muted .o_priority_star.fa-star-o" data-name="Kanban Priority Start" title="Edit: Kanban Priority Start"/>
                                </div>
                                <div class="o_kanban_inline_block dropdown o_mail_activity o_field_widget" name="activity_ids">
                                    <a class="dropdown-toggle o-no-caret o_activity_btn" data-toggle="dropdown" role="button">
                                        <span role="img" class="fa fa-lg fa-fw o_activity_color_default fa-clock-o"/>
                                    </a>
                                    <div class="dropdown-menu o_activity" role="menu"/>
                                </div>
                            </div>
                            <div class="oe_kanban_bottom_right item" data-class="oe_kanban_card_muted .oe_kanban_bottom_right" data-name="Kanban Card Footer Right" title="Edit: Kanban Card Footer Right">
                                <div class="o_clickable_m2x_avatar o_field_many2one_avatar o_field_widget o_quick_editable" name="user_id">
                                    <div class="o_m2o_avatar item" data-class="oe_kanban_card_muted .oe_kanban_bottom_right .o_m2o_avatar" data-name="Kanban Card Footer Avatar" title="Edit: Kanban Card Footer Avatar">
                                        <img src="/web/image/res.users/2/avatar_128"/>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="oe_clear"/>
                </div>
                Danger Card
                <div class="oe_kanban_color_0 oe_kanban_global_click oe_kanban_card d-flex flex-column o_kanban_record oe_kanban_card_danger item" data-class="oe_kanban_card_danger" data-name="Kanban Card Danger" title="Edit: Kanban Card Danger" tabindex="0" role="article">
                    <div class="o_dropdown_kanban dropdown">
                        <a class="dropdown-toggle o-no-caret btn" role="button" data-toggle="dropdown" data-display="static"  aria-label="Dropdown menu" title="Dropdown menu">
                            <span class="fa fa-ellipsis-v"/>
                        </a>
                    </div>
                    <div class="oe_kanban_content flex-grow-1">
                        <div class="o_kanban_record_title oe_kanban_details item" data-class="oe_kanban_card_danger .o_kanban_record_title" data-name="Kanban Card Record Title" title="Edit: Kanban Record Title">
                            <strong><span>Title</span></strong>
                        </div>
                        <div class="o_kanban_record_subtitle item" data-class="oe_kanban_card_danger .o_kanban_record_subtitle" data-name="Kanban Record Details" title="Edit: Kanban Record Details">
                            <span class="o_field_monetary o_field_number o_field_widget o_quick_editable" name="expected_revenue">$&amp;nbsp;00,000.00</span>
                        </div>
                        <div>
                            <div class="o_field_many2manytags o_field_widget o_kanban_tags" name="tag_ids">
                                <span class="o_tag o_tag_color_1"><span/>Tags</span>
                            </div>
                        </div>
                    </div>
                    <div class="oe_kanban_footer item" data-class="oe_kanban_card_danger .oe_kanban_footer" data-name="Kanban Card Footer" title="Edit: Kanban Card Footer">
                        <div class="o_kanban_record_bottom">
                            <div class="oe_kanban_bottom_left item" data-class="oe_kanban_card_danger .oe_kanban_bottom_left" data-name="Kanban Card Footer Left" title="Edit: Kanban Card Footer Left">
                                <div role="radiogroup" class="o_priority o_field_widget" name="priority" aria-label="Priority">
                                    <a role="radio" aria-checked="true" tabindex="0" data-index="1" class="o_priority_star fa fa-star item" data-class="oe_kanban_card_danger .o_priority_star.fa-star" data-name="Kanban Checked Priority Start" title="Edit: Kanban Checked Priority Start"/>
                                    <a role="radio" aria-checked="false" aria-label="High" tabindex="-1" data-index="2" class="o_priority_star fa fa-star-o item" data-class="oe_kanban_card_danger .o_priority_star.fa-star-o" data-name="Kanban Priority Start" title="Edit: Kanban Priority Start"/>
                                </div>
                                <div class="o_kanban_inline_block dropdown o_mail_activity o_field_widget" name="activity_ids">
                                    <a class="dropdown-toggle o-no-caret o_activity_btn" data-toggle="dropdown" role="button">
                                        <span role="img" class="fa fa-lg fa-fw o_activity_color_default fa-clock-o"/>
                                    </a>
                                    <div class="dropdown-menu o_activity" role="menu"/>
                                </div>
                            </div>
                            <div class="oe_kanban_bottom_right item" data-class="oe_kanban_card_danger .oe_kanban_bottom_right" data-name="Kanban Card Footer Right" title="Edit: Kanban Card Footer Right">
                                <div class="o_clickable_m2x_avatar o_field_many2one_avatar o_field_widget o_quick_editable" name="user_id">
                                    <div class="o_m2o_avatar item" data-class="oe_kanban_card_danger .oe_kanban_bottom_right .o_m2o_avatar" data-name="Kanban Card Footer Avatar" title="Edit: Kanban Card Footer Avatar">
                                        <img src="/web/image/res.users/2/avatar_128"/>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="oe_clear"/>
                </div>
            </div>
            <div class="o_column_quick_create">
                <div class="o_quick_create_folded item" data-class="o_column_quick_create .o_quick_create_folded" title="Edit: Kanban Quick Create" data-name="Kanban Quick Create">
                    <span class="o_kanban_add_column item" data-class="o_column_quick_create .o_quick_create_folded .o_kanban_add_column" title="Edit: Kanban Add Column section" data-name="Kanban Add Column section">
                        <i class="fa fa-plus" role="img" aria-label="Add column" title="Add column"/>
                    </span>
                    <span class="o_kanban_title item" data-class="o_column_quick_create .o_quick_create_folded .o_kanban_title" title="Edit: Kanban Quick Create Title" data-name="Kanban Quick Create Title">Add a Column</span>
                </div>
            </div>
        </div>
        Kanban ungrouped
        <div class="o_kanban_view o_kanban_mobile o_kanban_ungrouped" style="min-height: fit-content;">
            <div class="oe_kanban_card oe_kanban_global_click o_kanban_record item" data-class="o_kanban_view.o_kanban_ungrouped .oe_kanban_global_click" data-name="Kanban Card" title="Edit: Kanban Card" tabindex="0" role="article">
                <div class="o_kanban_record_top mb16 item" data-class="o_kanban_view.o_kanban_ungrouped .o_kanban_record_top" data-name="Kanban Top" title="Edit: Kanban Top">
