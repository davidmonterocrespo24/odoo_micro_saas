    <t t-name="ThemeStudio.search">
        Search view facet
        <div class="o_searchview">
            <div class="o_searchview_input_container item" title="Edit: Search view" data-name="Search view">
                <div tabindex="0" role="img" aria-label="search" class="o_searchview_facet" data-class="o_searchview_facet">
